{
  "name": "gatsby-source-directus",
  "version": "0.0.1",
  "description": "Source plugin for Gatsby to fetch data from Directus Headless CMS",
  "main": "gatsby-node.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  },
  "author": "Konrad Jarosinski <konrad@jarosinski.uk>",
  "license": "MIT",
  "dependencies": {
    "directus-sdk-javascript": "^2.5.1",
    "gatsby-node-helpers": "^0.1.3"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.0.5"
  }
}
